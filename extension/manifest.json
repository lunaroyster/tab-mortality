{
  "name": "Tab Mortality",
  "version": "0.0.1",
  "description": "Tabs are default dead",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/tab-mortality-16.png",
    "48": "icons/tab-mortality-48.png",
    "128": "icons/tab-mortality-128.png"
  },
  "permissions": ["tabs", "storage"]
}
